/* main */
  

main{
    /* if-else example 1 when if is true */
    
    int i1; i1 = 0;
    int i2; i2= 1;
    if(i1 and i2){
      
    } else {
       print "there is only one zero";
    }
    
    /* if-else example 1 when if is false  */
     
     int i3; i3 = 0;
     int i4; i4 = 0;
     if(i3 or i4){
      
    } else {
        print "all are zero";
    }
    
    }
    

